/*
***************************************************************************
*	函 数 名: BSP_ST16C554_CS1D
*	功能说明: 板级 ST16C554 CS1D 功能函数
*	形    参: 无
*	返 回 值: 无
***************************************************************************
*/
void BSP_ST16C554_CS1D_ISR(void)
{
    uint8_t ret, i, flag;
    if(GPIO_ReadInputDataBit(GPIOF, GPIO_Pin_9))
    {
        EXTI_ClearITPendingBit(EXTI_Line9);
        ret = BSP_ST16C554_CS1D_Read();
//		USART_SendData(USART1, ret);
		printf("%x\r\n", ret);

        switch(recSta)
        {
        case RC632_STA_SOP1:
					len = 0;
					for(i = 0; i < 100; i++) drbuf[i] = 0x00;
						if(ret == 0xaa)
						{
							drbuf[len++] = ret;
							recSta = RC632_STA_SOP2;
						}
            break;
        case RC632_STA_SOP2:
					if(ret == 0xbb)
					{
            drbuf[len++] = ret;
            recSta = RC632_STA_LEN1;
					}
					else if(ret == 0xaa) recSta = RC632_STA_SOP2;
					else recSta = RC632_STA_SOP1;
            break;
        case RC632_STA_LEN1:
            dataLen = ret;
            drbuf[len++] =  ret;
            recSta = RC632_STA_LEN2;
            break;
        case RC632_STA_LEN2:
						drbuf[len++] = ret;
            recSta = RC632_STA_DAT;
            break;
        case RC632_STA_DAT:
            drbuf[len++] = ret;
						if(drbuf[len-1] == 0xBB && drbuf[len-2] == 0xAA)
						{
							recSta = RC632_STA_LEN1;
							len = 2;
							break;
						}
            if(len == dataLen + 3)
                recSta = RC632_STA_FCS;
            break;
        case RC632_STA_FCS:
					recSta = RC632_STA_SOP1;
					flag = drbuf[4];
					for(i = 5; i < 3 + dataLen; i++)
					{
						flag ^= drbuf[i];
					}
					dataLen = 0;
					if(flag == ret)
					{
						drbuf[len++] = ret;
            rxflag = 1;
					}
        }
//				printf("%d %x %d\r\n", len, ret, rxflag);
//
//		switch(recSta)
//		{
//			case LF125K_STA_SOP:
//				drbuf[0] = 0x00;
//				recSta = LF125K_STA_DAT;
//				break;
//			case LF125K_STA_DAT:
//				drbuf[++len] = (uint8_t) ret;
//				if(len == 3) recSta = LF125K_STA_FCS;
//				break;
//			case LF125K_STA_FCS:
//				drbuf[++len] = (uint8_t) ret;
//				recSta = LF125K_STA_SOP;
//				rxflag = 1;
//				len = 0;
//				break;
//		}
